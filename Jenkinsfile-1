pipeline{

   agent none
   options{
   buildDiscarder(logRotator(numToKeepStr: '5'))
   }
   stages{
   
     stage('Scan'){
     
         steps{
           withSonarQubeEnv(installationName: 'sq1'){
             sh './mvn clean org.sonarsource.scanner.maven:sonar-plugin:3.9.0.2155:sonar'
           }
         }
     
     }
   }
}
