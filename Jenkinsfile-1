pipeline {
    agent any
    stages {
    
     stage('SCM') {
               steps {
                
                git 'https://github.com/LaboratorioExprivia/EsempioGitHub'
            }
              
       }
    
        stage('Build') {
            steps {
                def scannerHome = tool 'sonar-scanner'
                withSonarQubeEnv('sq1') {
                    sh "${scannerHome}/bin/sonar-scanner"
                }
            }
        }
    }
}
